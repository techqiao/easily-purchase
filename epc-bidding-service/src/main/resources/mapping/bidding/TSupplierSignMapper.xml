    SELECT
	sbf.supplier_id as supplierId,sbf.`name`,sbf.cellphone,sdi.company_name as companyName
FROM
	t_supplier_basic_info sbf
LEFT JOIN t_supplier_detail_info sdi on sdi.id=sbf.supplier_id and sdi.is_deleted=0
WHERE 1=1
and sbf.is_deleted= 0
and sbf.`name`=#{name}
and sbf.cellphone=#{cellphone}

