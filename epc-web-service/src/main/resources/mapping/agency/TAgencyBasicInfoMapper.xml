    <select id="queryAgencyByCrtiteria" resultMap="agencyMap"
            parameterType="com.epc.web.facade.agency.handle.HandleAgency">
        select b.name,b.agency_id as typeId,b.cellphone,
        d.company_name as companyName,d.uniform_credit_code as uniformCreditCode,d.public_bank_name as
        publicBankName,d.public_ban_account_number as publicBanAccountNumber,
        a.certificate_type as certificateType ,a.certificate_file_path as certificateFilePath,a.certificate_number as
        certificateNumber,a.certificate_name as certificateName
        from t_agency_basic_info b left join t_agency_detail_info d on b.agency_id=d.agency_id
        left join t_agency_attachment a on b.agency_id=a.agency_id
        group by d.company_name
        <where>
            b.role=0
            <if test="name!=null">
                AND b.`name` LIKE '%'#{name}'%'
            </if>
            <if test="agencyId!=null">
                AND b.agency_id=#{agencyId}
            </if>
            <if test="cellphone!=null">
                AND b.cellphone LIKE '%'#{cellphone}'%'
            </if>
            <if test="companyName!=null">
                d.company_name LIKE '%'#{companyName}'%'
            </if>
